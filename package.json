{
  "name": "@storybook/design-system",
  "version": "0.0.44",
  "description": "Storybook design system",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/design-system.git"
  },
  "license": "MIT",
  "author": {
    "name": "Michael Shilman",
    "email": "michael@lab80.co"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.production.cjs.js",
  "module": "dist/index.production.esm.js",
  "scripts": {
    "build": "yarn build:dev && yarn build:prod && yarn build:stories",
    "build-docs": "build-storybook --docs",
    "build-storybook": "build-storybook",
    "build:dev": "yarn rollup -c --environment BUILD:development",
    "build:prod": "yarn rollup -c --environment BUILD:production",
    "build:stories": "yarn rollup --config rollup.config.stories.js",
    "chromatic": "CHROMATIC_APP_CODE=9ofhn0iql7n chromatic test",
    "lint": "eslint .",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "prepublish": "yarn build:dev && yarn test && yarn build:prod",
    "release": "yarn prepublish && ./scripts/release.sh",
    "storybook": "start-storybook -p 6006",
    "test": "echo 'No tests exist yet'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "yarn lint:js --fix",
        "git add"
      ],
      "*.html": [
        "yarn lint:js --fix",
        "git add"
      ],
      "package.json": [
        "yarn lint:package",
        "git add"
      ]
    }
  },
  "dependencies": {
    "keycode": "^2.2.0",
    "polished": "^3.3.0",
    "prismjs": "1.13.0",
    "react-modal": "^3.8.1",
    "react-popper-tooltip": "^2.8.2",
    "recompose": "^0.30.0",
    "styled-components": "^4.1.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@innocuous/functions": "^2.0.1",
    "@storybook/addon-a11y": "^5.2.0-beta.32",
    "@storybook/addon-actions": "^5.2.0-beta.32",
    "@storybook/addon-docs": "^5.2.0-beta.32",
    "@storybook/addon-knobs": "^5.2.0-beta.32",
    "@storybook/addon-storysource": "^5.2.0-beta.32",
    "@storybook/addons": "^5.2.0-beta.32",
    "@storybook/react": "^5.2.0-beta.32",
    "@storybook/source-loader": "^5.2.0-beta.32",
    "auto": "^4.14.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.9.4",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rollup": "^1.20.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1",
    "sort-package-json": "^1.21.0",
    "storybook-chromatic": "^1.3.3"
  },
  "peerDependencies": {
    "prop-types": ">= 15.5.4",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "styled-components": ">= 4.1.2"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
